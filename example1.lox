class Animal {
    init() {
        this.x = 8;
    }

    bark() {
        print "...";
    }
}

class Dog < Animal {
    init() {
        this.x = 5;
    }

    bark() {
        print "Woof!";
    }

    supBark() {
        super.bark();
    }
}

var dog = Dog();
print dog.x;
dog.bark();
print dog.x;
dog.supBark();